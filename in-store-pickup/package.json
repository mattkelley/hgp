{
  "name": "hgp-in-store-pickup",
  "version": "1.0.0",
  "description": "In Store order pickup service for Hungry Ghost Press",
  "main": "src/updateOrder/updateOrder.controller.ts",
  "scripts": {
    "start": "sls offline start --port 3002 --apiKey 978testkey --printOutput",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rm -rf ./.webpack ./.serverless",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@types/config": "0.0.34",
    "axios": "0.18.1",
    "bunyan": "1.8.12",
    "class-transformer": "0.2.2",
    "class-validator": "0.9.1",
    "http-status-codes": "1.3.2",
    "lodash": "4.17.11",
    "reflect-metadata": "0.1.12",
    "serverless-offline": "4.10.0",
    "tsconfig-paths-webpack-plugin": "3.2.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.17",
    "@types/bunyan": "1.8.5",
    "@types/jest": "23.3.1",
    "@types/lodash": "4.14.121",
    "@types/node": "8.10.15",
    "@types/supertest": "2.0.5",
    "jest": "23.5.0",
    "nodemon": "1.18.3",
    "prettier": "1.14.2",
    "serverless-webpack": "5.2.0",
    "source-map-support": "0.4.3",
    "supertest": "3.1.0",
    "ts-jest": "23.1.3",
    "ts-loader": "4.2.0",
    "ts-node": "7.0.1",
    "tsconfig-paths": "3.5.0",
    "tslint": "5.11.0",
    "typescript": "3.3.1",
    "webpack": "4.5.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "author": "Matt Kelley",
  "license": "MIT"
}
